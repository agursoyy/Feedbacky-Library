{
  "name": "feedbacky-library",
  "version": "0.1.3",
  "description": "Feedbacky Library",
  "main": "./dist/feedbacky.js",
  "types": "./dist/types/App.d.ts",
  "scripts": {
    "eslint": "eslint --max-warnings 0 --ext .ts,.tsx,.jsx,.js .",
    "eslint:fix": "eslint --fix --ext .ts,.tsx,.jsx,.js .",
    "prettier": "prettier \"**/*.{ts,tsx,js,jsx,scss,html,json}\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write",
    "clear": "rm -rf dist",
    "dev": "webpack-dev-server --hot --open --env local",
    "predist": "npm run eslint && npm run prettier:check & npm run clear && npm run tsc",
    "dist": "webpack && npm run tsc:defs",
    "tsc": "tsc && npm run tsc:test",
    "tsc:test": "tsc --p tsconfig.test.json",
    "tsc:defs": "tsc --noEmit false --declaration --emitDeclarationOnly"
  },
  "keywords": [],
  "author": "Alptekin Gursoy",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.16.10",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.10",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.16.3",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "babel-jest": "27.4.6",
    "babel-loader": "8.2.3",
    "babel-preset-react-app": "10.0.1",
    "css-loader": "6.5.1",
    "eslint": "8.7.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "html-webpack-plugin": "5.5.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.4.7",
    "mini-css-extract-plugin": "2.5.2",
    "node-sass": "7.0.1",
    "prettier": "2.5.1",
    "sass-loader": "12.4.0",
    "style-loader": "3.3.1",
    "typescript": "4.5.5",
    "webpack": "5.66.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.3"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "7.16.7",
    "classnames": "2.3.1",
    "core-js": "3.20.3",
    "moment": "2.29.1",
    "preact": "10.6.5"
  }
}

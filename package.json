    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write",
    "clear": "rm -rf dist",
    "dev": "webpack-dev-server --hot --open --env local",
    "predist": "npm run eslint && npm run prettier:check & tsc",
    "dist": "npm run clear &&  webpack"
